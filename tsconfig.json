{
    "compilerOptions": {
        "module": "commonjs",
        "noImplicitAny": false,
        "removeComments": true,
        "noUnusedLocals": true,
        "preserveConstEnums": true,
        "allowJs": true,
        "sourceMap": false,
        "outDir": "build/express",
        "target": "es5",
        "esModuleInterop": true
    },
    "exclude": [
        "node_modules",
        "build"
    ]
}